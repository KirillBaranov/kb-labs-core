{
  "name": "@kb-labs/core-runtime",
  "version": "0.1.5",
  "description": "DI Container + Core Features Implementations for KB Labs Platform",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "dev": "tsup --config tsup.config.ts --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@kb-labs/core-contracts": "workspace:*",
    "@kb-labs/core-platform": "workspace:*",
    "@kb-labs/core-ipc": "workspace:*",
    "@kb-labs/core-resource-broker": "workspace:*",
    "@kb-labs/llm-router": "workspace:*",
    "@kb-labs/plugin-execution-factory": "link:../../../kb-labs-plugin/packages/plugin-execution-factory",
    "glob": "^11.0.0"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@types/node": "^24.3.3",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4",
    "rimraf": "^6.0.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "kb-labs",
    "platform",
    "runtime",
    "di",
    "container"
  ],
  "author": "KB Labs Team",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
