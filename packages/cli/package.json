{
  "name": "@kb-labs/core-cli",
  "version": "0.1.0",
  "type": "module",
  "description": "CLI commands for KB Labs core configuration system",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./manifest": {
      "import": "./dist/manifest.v2.js",
      "types": "./dist/manifest.v2.d.ts"
    }
  },
  "kb": {
    "manifest": "./dist/manifest.v2.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ignore-pattern 'dist/**'",
    "lint:fix": "eslint . --fix --ignore-pattern 'dist/**'"
  },
  "dependencies": {
    "@kb-labs/core-types": "workspace:*",
    "@kb-labs/cli-core": "link:../../../kb-labs-cli/packages/core",
    "@kb-labs/core-bundle": "link:../bundle",
    "@kb-labs/core-config": "link:../config",
    "@kb-labs/core-policy": "link:../policy",
    "@kb-labs/core-profiles": "link:../profiles",
    "@kb-labs/core-workspace": "link:../core",
    "@kb-labs/plugin-manifest": "link:../../../kb-labs-plugin/packages/manifest",
    "@kb-labs/shared-cli-ui": "link:../../../kb-labs-shared/packages/cli-ui",
    "glob": "^11.0.3",
    "picomatch": "^4.0.2",
    "uuidv7": "^0.6.1",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
