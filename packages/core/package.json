{
  "name": "@kb-labs/core",
  "version": "0.1.0",
  "description": "Core CLI commands for KB Labs - profiles, bundle, init, policy",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "kb": {
    "commandsManifest": "./dist/cli.manifest.js"
  },
  "author": {
    "name": "Kirill Baranov",
    "url": "https://github.com/kirill-baranov"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kirill-baranov/kb-labs-core.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/kirill-baranov/kb-labs-core/issues"
  },
  "homepage": "https://github.com/kirill-baranov/kb-labs-core#readme",
  "keywords": [
    "kb-labs",
    "cli",
    "profiles",
    "bundle",
    "init",
    "policy"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-pattern 'dist/**'",
    "lint:fix": "eslint . --fix --ignore-pattern 'dist/**'",
    "test": "vitest run -c ../../vitest.config.ts",
    "test:watch": "vitest -c ../../vitest.config.ts"
  },
  "dependencies": {
    "@kb-labs/core-bundle": "link:../bundle",
    "@kb-labs/core-config": "link:../config",
    "@kb-labs/core-policy": "link:../policy",
    "@kb-labs/core-profiles": "link:../profiles",
    "@kb-labs/shared-cli-ui": "link:../../../kb-labs-shared/packages/cli-ui"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@types/node": "^24.7.0",
    "tsup": "^8",
    "tsx": "^4.20.5",
    "vitest": "^3"
  }
}

