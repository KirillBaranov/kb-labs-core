{
  "name": "@kb-labs/core-cli",
  "version": "0.1.5",
  "type": "module",
  "description": "CLI commands for KB Labs core configuration system",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./plugin-manifest": {
      "types": "./dist/manifest.v2.d.ts",
      "import": "./dist/manifest.v2.js",
      "require": "./dist/manifest.v2.js"
    }
  },
  "kb": {
    "manifest": "./dist/manifest.v2.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ignore-pattern 'dist/**'",
    "lint:fix": "eslint . --fix --ignore-pattern 'dist/**'"
  },
  "dependencies": {
    "@kb-labs/cli-core": "link:../../../kb-labs-cli/packages/cli-core",
    "@kb-labs/core-bundle": "workspace:*",
    "@kb-labs/core-config": "workspace:*",
    "@kb-labs/core-contracts": "workspace:*",
    "@kb-labs/core-policy": "workspace:*",
    "@kb-labs/shared-cli-ui": "link:../../../kb-labs-shared/packages/shared-cli-ui",
    "@kb-labs/shared-command-kit": "link:../../../kb-labs-shared/packages/shared-command-kit",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
